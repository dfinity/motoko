// Relational operators

func testInt(a : Int, b : Int) {
   let eq1 = a == b;
   let eq2 = (a == b) : Bool;
   let neq1 = a != b;
   let neq2 = (a != b) : Bool;
   let lt1 = a < b;
   let lt2 = (a < b) : Bool;
   let le1 = a <= b;
   let le2 = (a <= b) : Bool;
   let gt1 = a > b;
   let gt2 = (a > b) : Bool;
   let ge1 = a >= b;
   let ge2 = (a >= b) : Bool;
};


func testBool(a : Bool, b : Bool) {
   let eq1 = a == b;
   let eq2 = (a == b) : Bool;
   let neq1 = a != b;
   let neq2 = (a != b) : Bool;
};


