type my_type = nat;
type B = my_type;

type message = record { 
  name : text;
  25 : B;
  reserved; 
};
type broker = service {
  find : (name : text) -> 
    (service {up:() -> (); current:() -> (nat)});
};

service server {
  addUser : (name : text, age : nat8) -> (id : nat64);
  userName : (id : nat64) -> (text) pure;
  userAge : (id : nat64) -> (nat8) pure;
  deleteUser : (id : nat64) -> () oneway;
}

