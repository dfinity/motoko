field claimed: Bool
field count: Int
define invariant_Actor(self) (((((self).claimed && (!(-1 == -1))) && (-42 == -42)) || true))
method claim($Self: Ref)  requires invariant_Actor($Self)
  ensures invariant_Actor($Self)
   { 
     if (!($Self).claimed)
        { 
          ($Self).claimed := true
          ($Self).count := 1 
        } 
   }
